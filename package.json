{
  "name": "@shinGangan/nuxt-markuplint",
  "version": "0.1.0",
  "description": "Markuplint module for Nuxt",
  "repository": "@shinGangan/nuxt-markuplint",
  "license": "MIT",
  "type": "module",
  "configKey": "markuplint",
  "compatibility": {
    "nuxt": ">=3.0.0",
    "bridge": false
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@markuplint/vue-parser": "^3.12.0",
    "@markuplint/vue-spec": "^3.13.0",
    "@nuxt/kit": "^3.8.2",
    "markuplint": "^3.15.0"
  },
  "devDependencies": {
    "@markuplint/ml-config": "^3.14.0",
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^0.5.4",
    "@nuxt/schema": "^3.8.2",
    "@nuxt/test-utils": "^3.8.1",
    "@types/node": "^20.10.3",
    "changelogen": "^0.5.5",
    "eslint": "^8.55.0",
    "nuxt": "^3.8.2",
    "vitest": "^0.34.6"
  }
}
